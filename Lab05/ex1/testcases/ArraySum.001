(* 错误类型：词法错误 - 非法标识符（包含数字开头） *)
MODULE ArraySum;

TYPE
  IntArray = ARRAY 5 OF INTEGER;

VAR
  arr: IntArray;
  i, sum: INTEGER;

BEGIN
  arr[0] := 1;
  arr[1] := 2;
  arr[2] := 3;
  arr[3] := 4;
  arr[4] := 5;

  sum := 0;
  i := 0;
  WHILE i < 5 DO
    sum := sum + arr[i];
    i := i + 1
  END
END 1ArraySum. (* 非法模块名：数字开头 *)