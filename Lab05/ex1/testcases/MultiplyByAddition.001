(* 错误类型：词法错误 - 非法标识符（包含数字开头） *)
MODULE MultiplyByAddition;

VAR
  a, b, product, i: INTEGER;

PROCEDURE Multiply(x, y: INTEGER): INTEGER;
VAR
  result, j: INTEGER;
BEGIN
  result := 0;
  j := 0;
  WHILE j < y DO
    result := result + x;
    j := j + 1
  END;
  RETURN result
END Multiply;

BEGIN
  a := 4;
  b := 3;
  product := Multiply(a, b)
END 1MultiplyByAddition. (* 非法模块名：数字开头 *)
