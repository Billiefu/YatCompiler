(* 错误类型：语法错误 - 数组索引缺少右括号 *)
MODULE ArraySum;

TYPE
  IntArray = ARRAY 5 OF INTEGER;

VAR
  arr: IntArray;
  i, sum: INTEGER;

BEGIN
  arr[0] := 1;
  arr[1 := 2; (* 缺少右括号 *)
  arr[2] := 3;
  arr[3] := 4;
  arr[4] := 5;

  sum := 0;
  i := 0;
  WHILE i < 5 DO
    sum := sum + arr[i];
    i := i + 1
  END
END ArraySum.