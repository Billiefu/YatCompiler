(* 错误类型：语法错误 - 缺少分号 *)
MODULE GCD;

VAR
  a, b, r, result: INTEGER;

PROCEDURE ComputeGCD(x, y: INTEGER): INTEGER;
VAR
  temp: INTEGER;
BEGIN
  WHILE y # 0 DO
    temp := y
    y := x MOD y; (* 缺少分号 *)
    x := temp
  END;
  RETURN x
END ComputeGCD;

BEGIN
  a := 48;
  b := 18;
  result := ComputeGCD(a, b)
END GCD.