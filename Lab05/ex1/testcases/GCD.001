(* 错误类型：词法错误 - 非法标识符（包含特殊字符） *)
MODULE GCD;

VAR
  a, b, r, result: INTEGER;

PROCEDURE ComputeGCD(x, y: INTEGER): INTEGER;
VAR
  temp: INTEGER;
BEGIN
  WHILE y # 0 DO
    temp := y;
    y := x MOD y;
    x := temp
  END;
  RETURN x
END ComputeGCD;

BEGIN
  a := 48;
  b := 18;
  result := ComputeGCD(a, b)
END GCD@. (* 非法模块名：包含特殊字符 '@' *)